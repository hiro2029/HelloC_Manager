{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "extends": ["plugin:jest/recommended", "eslint:recommended", "prettier"],
  "rules": {
    "react/jsx-uses-vars": 1,
    "no-unused-vars": "off"
  }
}
